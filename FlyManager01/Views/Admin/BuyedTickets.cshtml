@model IEnumerable<FlyManager01.Models.Person>
@*@model IEnumerable<FlyManagerVS.Models.Works>*@

@{
    ViewData["Title"] = "Account";
}

<h1>Accounts</h1>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>EGN</th>
            <th>Telephon</th>
            <th>Ticket</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MiddleName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EGN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TelephoneNummer)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TypeTicket)
                </td>
            </tr>
        }
    </tbody>
</table>